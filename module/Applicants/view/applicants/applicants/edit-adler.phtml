    function addQualifiedCourse(pkey)
    {
        $("#course-add-dialog").dialog({
           modal: true,
           autoOpen:true,
           width: 800,
           height: 500,
            buttons: {       
                "Save":  saveCourses, 
                Cancel: function() {
