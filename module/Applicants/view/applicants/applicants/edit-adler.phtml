    function saveCourses()
    {
        if ($(".courses_add_check:checkbox:checked").length > 0)
        {
           var form_input_data = $("form#course_add_form").serialize();
            $.ajax({
                url: "<?php echo $this->url('applicants',array('action'=>'saveQualifiedCourses'))?>",
                type: 'POST',                    
                data: form_input_data,                 
                success: function(response) {
                    window.location = "<?php echo $this->url('applicants',array('action'=>'edit','pkey'=>$pkey,'tab'=>3))?>";
                }
            });
        }
        else
        {
            alert("Please select any one checkbox.");            
        }        
    } 
    function removeCourses()
    {
        var sList = Array();
        if ($(".courses_remove_check:checkbox:checked").length > 0)
        {
            $(".courses_remove_check:checkbox:checked").each(function () {                
                sList.push($(this).val());
            });
        }
        else
        {
           alert("Please select any one checkbox.");
        }       
        if(sList.length>0)
        {
            var input_data = JSON.stringify(sList);            
            $.ajax({
                url: "<?php echo $this->url('applicants',array('action'=>'removeQualifiedCourses'))?>",
                type: 'POST',                    
                data: ({party_course_ids: input_data,Newapplication_pkey: '<?php echo $pkey;?>'}),                 
                success: function(response) {                    
                    window.location = "<?php echo $this->url('applicants',array('action'=>'edit','pkey'=>$pkey,'tab'=>3))?>";
                }
            });
        }        
    } 
    function removeDocs()
    {
        var sList = Array();
        var sType = Array();
        if ($(".docs_check:checkbox:checked").length > 0)
        {
            $(".docs_check:checkbox:checked").each(function () {                
                sList.push($(this).val());
                sType.push($(this).attr('name').replace("_check[]",""));
            });
        }
        else
        {
           alert("Please select any one checkbox.");
        }          
        if(sList.length>0)
        {
            var input_data = JSON.stringify(sList);  
            var doc_type = JSON.stringify(sType);  
            $.ajax({
                url: "<?php echo $this->url('applicants',array('action'=>'removeDocs'))?>",
                type: 'POST',                    
                data: ({docs_ids: input_data,docs_type: doc_type,Newapplication_pkey: '<?php echo $pkey;?>'}),                 
                success: function(response) {                    
                    window.location = "<?php echo $this->url('applicants',array('action'=>'edit','pkey'=>$pkey,'tab'=>1))?>";
                }
            });
        }
    }
    function candidateApproved()
    {
        var sList = Array();
        if ($("input[name='is_approved[]']:checkbox:checked").length > 0)
        {
            $("input[name='is_approved[]']:checkbox:checked").each(function () {                
                sList.push($(this).val());                
            });
        }  
        var input_data = JSON.stringify(sList);              
        $.ajax({
            url: "<?php echo $this->url('applicants',array('action'=>'saveCandidateApproved'))?>",
            type: 'POST',                    
            data: ({party_course_ids: input_data,Newapplication_pkey: '<?php echo $pkey;?>'}),                 
            success: function(response) {                    
                window.location = "<?php echo $this->url('applicants',array('action'=>'edit','pkey'=>$pkey,'tab'=>3))?>";
            }
        });
         
    } 
    function IsRequired(pkey,value){        
        var expiration_class = 'expi_date_'+pkey;
        if(value==1){
            $("."+expiration_class).attr('required',true);
        } else {
            $("."+expiration_class).attr('required',false);
        }    
    }
    function IsRequiredNew(value){        
        var expiration_class = 'expi_date';
        if(value==1){
            $("."+expiration_class).attr('required',true);
        } else {
            $("."+expiration_class).attr('required',false);
        }    
    }
    
